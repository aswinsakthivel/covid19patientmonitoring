<%
   String age=request.getParameter("age");
   session.setAttribute("sessage", age);
   String temp=request.getParameter("temper");
   session.setAttribute("sesstemp", temp);
   String pulse=request.getParameter("pulse");
   session.setAttribute("sesspulse", pulse);
   String heart=request.getParameter("heart");
   session.setAttribute("sessheart", heart);
  try{
 Class.forName("com.mysql.jdbc.Driver");
 Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/mydata3","root","Aswin@123");
 Statement st=con.createStatement();
      int i=st.executeUpdate("insert into checker(temp,pulse,heart,age)values('"+temp+"','"+pulse+"','"+heart+"','"+age+"')");
      Statement st1=con.createStatement();
      String myname=(String)session.getAttribute("sessname");
      String myemail=(String)session.getAttribute("sessemail");
      String myphone=(String)session.getAttribute("sessnumb");
      String myage=(String)session.getAttribute("sessage");
      String mytemp=(String)session.getAttribute("sesstemp");
      String myheart=(String)session.getAttribute("sessheart");
      String mypulse=(String)session.getAttribute("sesspulse");
      int j=st1.executeUpdate("insert into alldetails(name,email,phonenum,age,temp,heart,pulse)values('"+myname+"','"+myemail+"','"+myphone+"','"+myage+"','"+mytemp+"','"+myheart+"','"+mypulse+"')");
      out.println("Data is successfully inserted!");
 response.sendRedirect("save.html");
  }
  catch(Exception e)
  {
 out.print("Database error");
  }
%>
</body>
</html>
