            function cal(){
            var tot=0;
            //var tot = document.getElementById("answer");
                //ansD.value = val1 + val2;
            var t=document.querySelector('input[name="fevercheck"]:checked').value;
                if(t=="yes")
                {
                tot=tot+20;
                }
                var t1=document.querySelector('input[name="coldcheck"]:checked').value;
                if(t1=="yes")
                {
                tot=tot+20;
                }
                var t2=document.querySelector('input[name="tiredcheck"]:checked').value;
                if(t2=="yes")
                {
                tot=tot+20;
                }
                var t3=document.querySelector('input[name="breathcheck"]:checked').value;
                if(t3=="yes")
                {
                tot=tot+20;
                }
                var t4=document.querySelector('input[name="headcheck"]:checked').value;
                if(t4=="yes")
                {
                tot=tot+20;
                }
                //document.write(tot);
                if(tot >= 60)
                {
                alert("BE STRONG YOU WILL CURED SOON!! LETS PROCEED FURTHER");
                window.location.href="check.jsp";
                }else
                {
                alert("HAPPY THAT YOU ARE RECOVERING!! LETS PROCEED FURTHER");
                window.location.href="check.jsp";
                }
            }
