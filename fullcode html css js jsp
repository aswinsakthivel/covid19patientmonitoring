        <script>
        function calculate(){
        var t1=document.getElementById("temp").value;
            var pulse=document.getElementById("pulserate").value;
            var heart=document.getElementById("heartrate").value;
            var age=document.getElementById("age").value;
            window.location.href=" ";
            if(t1 >= 97 && t1<=101)
            {
            document.write("TEMPERATURE IS NORMAL");
            document.write("<br />");
            }
            if(pulse >=60 && pulse<=100)
            {
            document.write("PULSE IS NORMAL\n");
            document.write("<br />");
            }
            if(age<=1)
            {
            if(heart>=120 && heart<=160)
                {
            document.write("BABY'S HEART RATE IS NORMAL\n");
                }else
                {
                document.write("BABY'S HEART RATE IS ABNORMAL\n");
                }
            }else if(age<=2)
            {
            if(heart>=90 && heart<=140)
            {
            document.write("BABY'S HEART RATE IS NORMAL\n");
            }else
            {
            document.write("BABY'S HEART RATE IS ABRNOMAL\n");
            }
           
            }else if(age<=6)
            {
            if(heart>=80 && heart<=110)
            {
            document.write("CHILD'S HEART RATE IS NORMAL\n");
            }else
            {
            document.write("CHILD'S HEART RATE IS ABNORMAL\n");
            }
            }else if(age<=12)
            {
            if(heart>=75 && heart<=100)
            {
            document.write("CHILD'S HEART RATE IS NORMAL");
            }else
            {
            document.write("CHILD'S HEART RATE IS ABNORMAL");
            }
            }else if(age<=19)
            {
            if(heart>=60 && heart<=90)
            {
            document.write("ADULT HEART RATE IS NORMAL");
            }else
            {
            document.write("ADULT HEART RATE IS ABNORMAL");
            }
            }else
            {
            if(heart<=60 && heart<=100)
            {
            document.write("HEART RATE OF THE PERSON IS NORMAL");
            }else
            {
            document.write("HEART RATE OF THE PERSON IS ABNORMAL");
            }
            }
           
        }
        </script>
</body>
</html> 
